              <stringProp name="filename"></stringProp>
              <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils;

//Script to generate random phone number

// Generate a random 9-digit number
String randomNumber = RandomStringUtils.randomNumeric(9);

// Add &apos;9&apos; at the beginning of the number
String phoneNumber = &quot;9&quot; + randomNumber;

// Set the generated phone number into a JMeter variable
vars.put(&quot;randomPhoneNumber&quot;, phoneNumber);

// List of random first names
String[] firstNames = {
    &quot;John&quot;, &quot;Jane&quot;, &quot;Michael&quot;, &quot;Emily&quot;, &quot;William&quot;, &quot;Olivia&quot;, &quot;David&quot;, &quot;Sophia&quot;
};

// List of random last names
String[] lastNames = {
    &quot;Smith&quot;, &quot;Johnson&quot;, &quot;Brown&quot;, &quot;Davis&quot;, &quot;Lee&quot;, &quot;Wilson&quot;, &quot;Taylor&quot;, &quot;Moore&quot;
};

// Generate a random index for first and last names
int randomFirstNameIndex = (int) (Math.random() * firstNames.length);
int randomLastNameIndex = (int) (Math.random() * lastNames.length);

// Get a random first and last name
String randomFirstName = firstNames[randomFirstNameIndex];
String randomLastName = lastNames[randomLastNameIndex];

// Set the generated first and last names into JMeter variables
vars.put(&quot;randomFirstName&quot;, randomFirstName);
vars.put(&quot;randomLastName&quot;, randomLastName);
</stringProp>
