            <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Generate Test Data" enabled="true">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import java.util.Random;

// Define an array of first names
String[] firstNames = {&quot;John&quot;, &quot;Jane&quot;, &quot;Michael&quot;, &quot;Emily&quot;, &quot;David&quot;, &quot;Sarah&quot;, &quot;Robert&quot;, &quot;Olivia&quot;};
String[] lastNames = {&quot;Smith&quot;, &quot;Johnson&quot;, &quot;Williams&quot;, &quot;Jones&quot;, &quot;Brown&quot;, &quot;Davis&quot;, &quot;Miller&quot;, &quot;Wilson&quot;};

// Create a random number generator
Random random = new Random();

// Generate a random index to select a first name
int randomIndex1 = random.nextInt(firstNames.length);
int randomIndex2 = random.nextInt(lastNames.length);
// Get the random first name
String randomFirstName = firstNames[randomIndex1];
String randomLastName = lastNames[randomIndex2];
// Set the generated first name to a JMeter variable
vars.put(&quot;FirstName&quot;, randomFirstName);
vars.put(&quot;LastName&quot;, randomLastName);
// Print the generated first name to the JMeter log
log.info(&quot;Random First Name: &quot; + randomFirstName);
log.info(&quot;Random Last Name: &quot; + randomLastName);


// Generate a random 10-digit phone number
StringBuilder phoneNumber = new StringBuilder(&quot;0&quot;); // The first digit is usually the country code
for (int i = 1; i &lt; 10; i++) {
    phoneNumber.append(random.nextInt(10));
}

// Set the generated phone number to a JMeter variable
vars.put(&quot;Mobile&quot;, phoneNumber.toString());

// Print the generated phone number to the JMeter log
log.info(&quot;Random Phone Number: &quot; + phoneNumber.toString());

// Define the Gmail domain
String gmailDomain = &quot;@gmail.com&quot;;


// Generate a random username (e.g., randomstring123)
String username = &quot;randomstring&quot; + random.nextInt(10000);

// Generate a random Gmail email address
String randomEmail = username + gmailDomain;

// Set the generated email address to a JMeter variable
vars.put(&quot;email&quot;, randomEmail);

// Print the generated email address to the JMeter log
log.info(&quot;Random Gmail Email: &quot; + randomEmail);

//Choosing genders

String[] genders = {&quot;Male&quot;, &quot;Female&quot;};
int randomIndex3 = random.nextInt(genders.length);
String randomGender = genders[randomIndex3];
vars.put(&quot;gender&quot;, randomGender);
log.info(&quot;Random Gender: &quot; + randomGender);

//Choosing Random Profession

String[] professions = {&quot;Doctor&quot;, &quot;Engineer&quot;, &quot;Student&quot;,&quot;Scientist&quot;};
int randomIndex4 = random.nextInt(professions.length);
String randomProfession = professions[randomIndex4];
vars.put(&quot;profession&quot;, randomProfession);
log.info(&quot;Random Profession: &quot; + randomProfession);

//Generating random password

String uppercaseLetters = &quot;ABCDEFGHIJKLMNOPQRSTUVWXYZ&quot;;
String lowercaseLetters = &quot;abcdefghijklmnopqrstuvwxyz&quot;;
String specialCharacters = &quot;!@#$%^&amp;*&quot;;
String numbers = &quot;0123456789&quot;;

StringBuilder password = new StringBuilder();

// Generate at least one uppercase letter
password.append(uppercaseLetters.charAt(random.nextInt(uppercaseLetters.length())));

// Generate at least one lowercase letter
password.append(lowercaseLetters.charAt(random.nextInt(lowercaseLetters.length())));

// Generate at least one special character
password.append(specialCharacters.charAt(random.nextInt(specialCharacters.length())));

// Generate at least one number
password.append(numbers.charAt(random.nextInt(numbers.length())));

// Generate the remaining characters for an 8-character password
int remainingLength = 8 - password.length();
for (int i = 0; i &lt; remainingLength; i++) {
    String charSet = uppercaseLetters + lowercaseLetters + specialCharacters + numbers;
    password.append(charSet.charAt(random.nextInt(charSet.length())));
}

// Shuffle the characters in the password to randomize the order
char[] passwordArray = password.toString().toCharArray();
for (int i = 0; i &lt; passwordArray.length; i++) {
    int randomIndex = random.nextInt(passwordArray.length);
    char temp = passwordArray[i];
    passwordArray[i] = passwordArray[randomIndex];
    passwordArray[randomIndex] = temp;
}

// Set the generated password to a JMeter variable
vars.put(&quot;pwd&quot;, new String(passwordArray));



